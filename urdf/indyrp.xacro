    <link name = "tcp" />

    <joint name = "global" type = "fixed">
    	<parent link = "ground"/>
    	<child link = "link0"/>
    	<origin xyz = "0 0 0" rpy = "0 0 0"/>
    </joint>

    <joint name = "joint0" type = "continuous">
    	<parent link = "link0"/>
    	<child link = "link1"/>
    	<origin xyz = "0 -0 0.1834" rpy = "0 0 0"/>
    	<axis xyz = "0 0 1"/>
    </joint>

    <joint name = "joint1" type = "continuous">
    	<parent link = "link1"/>
    	<child link = "link2"/>
    	<origin xyz = "0 0.098 0.08" rpy = "-${PI/2} 0 0"/>
